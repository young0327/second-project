<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.project.mapper.BoardMapper">

    <select id="boardList" resultType="kr.project.domain.Board" parameterType="String">
       select * from tsboard where category like CONCAT('%',#{category},'%')
    </select>
</mapper>